<body>


  <div id="mapid"></div>
  <form id="reportForm">
      <div class = "description">Description: </div>
      <input type = "text" name="descr"><br>
      <div class = "danger">Danger: </div>
      <select id="dang" name="danger">
        <option value="Low">S</option>
        <option value="Medium">M</option>
        <option value="High">L</option>
      </select><br>
      <button id="sendSign">SEND</button>
    </form>
    <form id="reportForm1" action="api/upload" enctype="multipart/form-data" method="POST">
